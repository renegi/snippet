{
  "version": 2,
  "builds": [
    {
      "src": "podquote/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/podquote/client/build/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg))",
      "dest": "/podquote/client/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/podquote/client/build/index.html"
    }
  ]
} 